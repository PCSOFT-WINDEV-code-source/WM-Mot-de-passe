  procedures :
   -
     name : ChangePlan
     procedure_id : 1685141100228916793
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //ChangePlan (<nNouveauPlan> est entier)
      //
      // Paramètres :
      //	nNouveauPlan (entier) :<indiquez ici le rôle de PLAN_DEFINITION>
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE ChangePlan( nNouveauPlan est un entier )
      
      MaFenêtre..Plan = nNouveauPlan
      
      selon nNouveauPlan
      	cas PLAN_ACCUEIL
      		
      	CAS PLAN_CHOIX_SECURE
      	CAS PLAN_DEFINIR_DESSIN
      		CMOD_DessinSecure.InitialiseChamp()
      		
      	CAS PLAN_CONFIRMER_DESSIN
      		CMOD_DessinSecure.InitialiseChamp()
      		
      	CAS PLAN_DEFINIR_MOTDEPASSE
      		SAI_MotDePasseCreation1 = ""
      		DonneFocus(SAI_MotDePasseCreation1)
      		
      	CAS PLAN_CONFIRMER_MOTDEPASSE
      		SAI_MotDePasseCreation2 = ""
      		DonneFocus(SAI_MotDePasseCreation2)
      		
      	CAS PLAN_SAISIR_DESSIN
      		CMOD_DessinSecure.InitialiseChamp()
      		
      	CAS PLAN_SAISIR_MOTDEPASSE
      		SAI_MotDePasseOuverture = ""
      		DonneFocus(SAI_MotDePasseOuverture)
      		
      	CAS PLAN_DEVERROUILLER
      		// Laisse passer un peu de temps pour que l'utilisateur puisse voir l'animation
      		<si CibleExécution<>MacCatalyst>
      		Multitâche(-100)
      		<fin>
      		OuvreFenêtreMobile(FEN_ListeMotDePasse)		
      fin
      
     type : 458752
   -
     name : Initialisation
     procedure_id : 1686977851647966467
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //Initialisation ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Initialisation()
      
      // Affiche la saisie du mot de passe général ou sa définition
      SELON ModeAuthentificationPrincipal()
      	CAS MODE_AUTH_PATTERN
      		ChangePlan(PLAN_SAISIR_DESSIN)
      	CAS MODE_AUTH_PASS
      		ChangePlan(PLAN_SAISIR_MOTDEPASSE)
      	AUTRE CAS
      		ChangePlan(PLAN_ACCUEIL)		
      FIN
