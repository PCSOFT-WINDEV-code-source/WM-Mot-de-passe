            //Exécute le traitement défini dans le modèle
            ExécuteAncêtre
            
            selon MaFenêtre..plan
            	cas PLAN_DEFINIR_DESSIN
            		// Si le motif contient moins de 4 points, il est trop court
            		// On affiche un toast et on recommence
            		si gtabPatternOrdre..Occurrence<4 alors
            			ToastAffiche("Le motif doit comporter au moins 4 points.")
            			InitialiseChamp()
            		FIN
            		
            	cas PLAN_CONFIRMER_DESSIN
            		// Si le deuxième dessin est identique au premier, on crée la base de données en la chiffrant avec ce dessin
            		si CMOD_DessinSecure.PatternVersChaine()=gsMotDePasseDéfinition ALORS
            			si CréeBaseDeDonnées(CMOD_DessinSecure.PatternVersChaine()) alors
            				SauveParamètre(PARAM_MODE_AUTH,MODE_AUTH_PATTERN)
            				ChangePlan(PLAN_DEVERROUILLER)
            			sinon
            				erreurasynchrone("Une erreur est survenue lors de la création de la base de données.","Merci de contacter le concepteur de l'application.")
            				ferme()
            			fin
            		sinon
            			ToastAffiche("Le dessin et sa confirmation ne correspondent pas.")
            			ChangePlan(PLAN_CHOIX_SECURE)
            		FIN
            	cas PLAN_SAISIR_DESSIN
            		// Tente d'ouvrir la base de données avec le dessin fourni
            		si OuvreBaseDeDonnées(CMOD_DessinSecure.PatternVersChaine(),vrai) ALORS
            			ChangePlan(PLAN_DEVERROUILLER)
            		sinon
            			ToastAffiche("Dessin de déverrouillage incorrect."+rc+"Veuillez réessayer")
            			CMOD_DessinSecure.InitialiseChamp()
            		FIN
            FIN
