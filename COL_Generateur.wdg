      // Résumé : Génération d'un mot de passe selon l'algorithme "Extrême"
      // Syntaxe :
      //[ <Résultat> = ] AutoPass_Extreme ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	chaîne UNICODE : Mot de passe
      //
      //	stInfo (STInfoLogin) : Paramètres de génération
      PROCEDURE AutoPass_Extreme() : chaine
      
      sMDP est une chaine
      bufHashUser est un buffer = _CalculeHashBase()
      nPtrDebut est un entier = 1
      
      bMaj est un booléen
      bMin est un booléen
      bChiffre est un booleen
      bSymbole est un booleen
      
      POUR n=0 _a_ 6
      	// Lit dans le buffer les 4 caractères suivants, les convertit en un entier et ramène la valeur dans les bornes du tableau des triplets
      	nElem est un entier = (_BufferVersEntier(bufHashUser,nPtrDebut,4) modulo gnNbTriplet)+1
      	nPtrDebut += 4
      	
      	// Lit dans le buffer un octet, sa valeur va déterminer si les éléments du triplet seront utilisés en majuscule, minuscule, chiffre, symboles ou espaces
      	nCasse est un entier = _BufferVersEntier(bufHashUser,nPtrDebut,1)
      	nPtrDebut++
      	sFragment est une chaîne = gtabTriplet[nElem]	
      	
      	// Les deux premiers bits déterminent le type du premier caractère ajouté (0 : majuscule, 1-2 : minuscule, 3 : chiffre)
      	nPremier est un entier = bitDécaleDroite(ETBinaire(nCasse,0xc0),6)		// 0b11000000
      	// Si au cinquième tour, on n'a toujours pas de majuscule, force la majuscule
      	SI n=5 _ET_ bMaj=Faux ALORS nPremier = 0
      	SELON nPremier
      		CAS 0
      			sMDP += Majuscule(sFragment[[1]])
      			bMaj = Vrai
      		CAS 1,2
      			sMDP += Minuscule(sFragment[[1]])
      			bMin = Vrai
      		CAS 3
      			sMDP += Asc(Minuscule(sFragment[[1]])) modulo 10
      			bChiffre = Vrai
      	FIN
      	
      	// Les trois bits suivants déterminent le type du second caractère ajouté
      	nSecond est un entier = bitDécaleDroite(ETBinaire(nCasse,0x38),3)		// 0b00111000
      	// Si au troisième tour, on n'a toujours pas de minuscule, force la minuscule
      	SI n=3 _ET_ bMin=Faux ALORS nPremier = 2
      	// Si au quatrième tour, on n'a toujours pas de chiffre, force le chiffre
      	SI n=4 _ET_ bChiffre=Faux ALORS nPremier = 4
      	SELON nSecond
      		CAS 0,1
      			sMDP += Majuscule(sFragment[[2]])
      			bMaj = Vrai
      		CAS 2,3
      			sMDP += Minuscule(sFragment[[2]])
      			bMin = Vrai
      		CAS 4,5
      			sMDP += Asc(Minuscule(sFragment[[2]])) modulo 10
      			bChiffre = Vrai
      		CAS 6,7
      			sMDP += gtabSymboles[(Asc(Minuscule(sFragment[[2]])) modulo gnNbSymboles)+1]
      			bSymbole = Vrai
      	FIN
      	
      	// Les trois derniers bits déterminent le type du troisième caractère ajouté
      	nTroisième est un entier = ETBinaire(nCasse,0x07)	// 0b00000111
      	// Si au deuxième tour, on n'a toujours pas de symbole, force le symbole
      	SI n=2 _ET_ bSymbole=Faux ALORS nPremier = 4
      	SELON nTroisième
      		CAS 0,1
      			sMDP += Majuscule(sFragment[[3]])
      			bMaj = Vrai
      		CAS 2,3
      			sMDP += Minuscule(sFragment[[3]])
      			bMin = Vrai
      		CAS 4,5
      			sMDP += Asc(Minuscule(sFragment[[3]])) modulo 10
      			bChiffre = Vrai
      		CAS 6,7
      			sMDP += gtabSymboles[(Asc(Minuscule(sFragment[[3]])) modulo gnNbSymboles)+1]
      			bSymbole = Vrai
      	FIN
      FIN
      
